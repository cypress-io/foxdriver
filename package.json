{
  "name": "@cypress/foxdriver",
  "version": "0.0.0-development",
  "description": "Foxdriver is a Node library which provides a high-level API to control Firefox over the Remote Debugging Protocol",
  "keywords": [
    "firefox",
    "remote",
    "debugging",
    "debug",
    "protocol",
    "cdp",
    "devtools"
  ],
  "homepage": "https://github.com/cypress-io/foxdriver#readme",
  "bugs": {
    "url": "https://github.com/cypress-io/foxdriver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/foxdriver.git"
  },
  "license": "Apache-2.0",
  "author": "Cypress.io",
  "main": "build/index.js",
  "scripts": {
    "build": "run-s clean compile",
    "ci": "run-s eslint test:unit:cover",
    "clean": "rm -rf ./build",
    "compile": "babel lib/ -d build/ --ignore lib/config --source-maps inline",
    "eslint": "eslint ./lib/**/*.js",
    "lint": "eslint --ext json,ts,js .",
    "release": "np patch",
    "release:major": "np major",
    "release:minor": "np minor",
    "release:patch": "np patch",
    "test": "run-s test:*",
    "test:e2e": "jest 'attach.test.js' --runInBand",
    "test:unit": "jest 'test/unit'",
    "test:unit:cover": "yarn test:unit --coverage",
    "watch": "npm run compile -- --watch"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 72,
        "functions": 93,
        "lines": 89,
        "statements": 89
      }
    }
  },
  "dependencies": {
    "fs-extra": "^4.0.1",
    "get-port": "^3.2.0",
    "npmlog": "^4.1.2",
    "safe-buffer": "^5.1.1",
    "tcp-port-used": "^1.0.1",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "firefox-profile": "^1.2.0",
    "geckodriver": "^1.19.1",
    "jest": "^24.9.0",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "sinon": "^3.2.0",
    "webdriverio": "^5.18.0"
  }
}
